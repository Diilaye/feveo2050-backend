# Configuration pour les tests API GIE
# Copiez ce fichier en .env.test et modifiez les valeurs selon vos besoins

# URL de base de l'API
API_BASE_URL=http://localhost:5000/api

# Utilisateur de test pour l'authentification
TEST_USER_EMAIL=admin@feveo.sn
TEST_USER_PASSWORD=password123

# Configuration des tests
TEST_CLEANUP=false  # Supprimer automatiquement les données de test après les tests
TEST_VERBOSE=true   # Affichage détaillé des logs

# Données de test par défaut
TEST_GIE_PREFIX=TEST-
TEST_PROTOCOL_PREFIX=PROT-TEST-

# Délais et timeouts (en millisecondes)
REQUEST_TIMEOUT=5000
AUTH_TIMEOUT=3000

# Base de données de test (optionnel)
TEST_DB_NAME=feveo_test
