  "guide_utilisation": {
    "ajouter_membre": {
      "endpoint": "POST /api/wallet/members/{gieCode}/add",
      "champs_obligatoires": ["nom", "prenom", "telephone"],
      "champs_optionnels": ["role", "dateNaissance", "profession", "adresse"],
      "limite": "40 membres maximum par GIE"
    },
    "modifier_membre": {
      "endpoint": "PUT /api/wallet/members/{gieCode}/{membreId}",
      "description": "Tous les champs sont optionnels, seuls les champs fournis seront modifiés"
    },
    "supprimer_membre": {
      "endpoint": "DELETE /api/wallet/members/{gieCode}/{membreId}",
      "description": "Suppression définitive du membre"
    },
    "consulter_membres": {
      "endpoint": "GET /api/wallet/members/{gieCode}",
      "description": "Liste complète des membres avec leurs informations"
    },
    "statistiques": {
      "endpoint": "GET /api/wallet/members/{gieCode}/stats",
      "description": "Statistiques détaillées sur les membres du GIE"
    }
  },
  "validations": {
    "telephone": "Unique dans le GIE, ne peut pas être dupliqué",
    "nom_prenom": "Obligatoires, espaces supprimés automatiquement",
    "limite_membres": "Maximum 40 membres par GIE",
    "acces": "Seuls les GIE avec adhésion peuvent gérer leurs membres"
